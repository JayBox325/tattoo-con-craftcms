# This file should be renamed to '.gitignore' and placed in your
# Craft CMS project root directory

# CRAFT ENVIRONMENT
.env.php
.env.sh
.env
.env.local

# COMPOSER
/vendor

# CRAFT STORAGE
/public/cpresrouces/
/web/cpresrouces/

# ASSETS
/public/uploads/*
/web/uploads/*

# IMAGER TRANSFORMS
/public/imager/*
/web/imager/*

# BUILD FILES
/bower_components/*
/node_modules/*
/yarn-error.log

# MISC FILES
.cache
.DS_Store
.idea
.project
.settings
*.esproj
*.sublime-workspace
*.sublime-project
*.tmproj
*.tmproject
.vscode/*
!.vscode/settings.json
!.vscode/tasks.json
!.vscode/launch.json
!.vscode/extensions.json
config.codekit3
prepros-6.config

# Prevent to .sql files (added by fortrabbit/craft-copy)
*.sql

/config/license.key
